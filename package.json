{
  "name": "macroiq",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "supabase:update-schema": "supabase db pull && supabase migration up",
    "supabase:update-remote-schema": "supabase db pull --local && supabase migration up --linked",
    "supabase:generate-types-local": "npx supabase gen types typescript --local > src/types/supabase.ts",
    "supabase:generate-types": "npx supabase gen types typescript --project-id cmxrncvgemkauyvthvyb > src/types/supabase.ts",
    "supabase:diff-storage": "supabase db diff --schema storage",
    "supabase:dump-storage": "supabase db dump --data-only --schema storage -f supabase/seed.sql",
    "supabase:db-reset": "supabase db reset",
    "supabase:functions-serve": "supabase functions serve --env-file=.env.local"
  },
  "jest": {
    "preset": "jest-expo"
  },
